{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "{ConnectionStrings:DefaultConnection}"
  },
  "Authentication": {
    "Secret": "{UserSecret}"
  },
  "IpRateLimiting": {
    "EnableEndpointRateLimiting": true, //When false, the rules apply globally
    "StackBlockedRequests": false,
    "RealIpHeader": "X-Real-IP",
    "ClientIdHeader": "X-ClientId",
    "HttpStatusCode": 429,
    "GeneralRules": [
      {
        "Endpoint": "*",
        "Period": "1s",
        "Limit": 1
      },
      {
        "Endpoint": "post:*",
        "Period": "10s",
        "Limit": 10
      }
    ]
  }
  
}