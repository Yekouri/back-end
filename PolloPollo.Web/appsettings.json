{
    "Logging": {
        "LogLevel": {
            "Default": "Warning"
        }
    },
    "AllowedHosts": "*",
    "ConnectionStrings": {
        "DefaultConnection": "{ConnectionStrings:DefaultConnection}"
    },
    "Authentication": {
        "Secret": "{UserSecret}"
    },
    "IpRateLimiting": {
        "EnableEndpointRateLimiting": true,
        "StackBlockedRequests": false,
        "RealIpHeader": "X-Real-IP",
        "ClientIdHeader": "X-ClientId",
        "HttpStatusCode": 429,
        "GeneralRules": [
            {
                "Endpoint": "*:/api/Users",
                "Period": "1s",
                "Limit": 2
            },
            {
                "Endpoint": "post:/api/Users",
                "Period": "10m",
                "Limit": 1
            },
            {
                "Endpoint": "*",
                "Period": "10m",
                "Limit": 1000
            }
        ]
    }
}