{
    "Logging": {
        "LogLevel": {
            "Default": "Warning"
        }
    },
    "AllowedHosts": "*",
    "ConnectionStrings": {
        "DefaultConnection": "{ConnectionStrings:DefaultConnection}"
    },
    "Authentication": {
        "Secret": "{UserSecret}"
    },
    "IpRateLimiting": {
        "EnableEndpointRateLimiting": true,
        "StackBlockedRequests": false,
        "RealIpHeader": "X-Real-IP",
        "ClientIdHeader": "X-ClientId",
        "HttpStatusCode": 429,
        // Rules not locked, needs to be tested first
        "GeneralRules": [
            
            {
                "Endpoint": "post:/api/Users",
                "Period": "5m",
                "Limit": 1
            }
            
        ]
    }
}